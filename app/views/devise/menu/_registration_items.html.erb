<!-- # from https://github.com/plataformatec/devise/wiki/How-To:-Add-sign_in,-sign_out,-and-sign_up-links-to-your-layout-template -->

<% if user_signed_in? %>
<li id='logInText-email'>
    Hi, <%= current_user.email %>
</li>
<br>
<li>
 	<span class='editAccount'>
 	<%= link_to('Edit Account', edit_user_registration_path) %>
 	</span>
</li>
<% else %>
<li>
  	<span class='SignUp'><%= link_to('Sign Up', new_user_registration_path) %>
</li>
<% end %>
